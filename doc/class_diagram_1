class RepositoryConfigurationDelegate
interface RepositoryConfigurationExtension
interface RepositoryConfigurationSource
class AnnotationRepositoryConfigurationSource
class MongoRepositoryConfigurationExtension
class DefaultRepositoryConfiguration
class RepositoryConfigurationSourceSupport
class AnnotationRepositoryConfigurationSource
class MongoRepositoryFactoryBean
class RepositoryBeanDefinitionBuilder
class RepositoryFactoryBeanSupport {
  -customImplementation
  -queryLookupStrategyKey
  -lazyInit
  -repositoryBaseClass
  -namedQueries
  -customImplementation
  -repositoryFragments
  -evaluationContextProvider
}
interface RepositoryFactoryInformation
interface FactoryBean


RepositoryConfigurationDelegate ..> MongoRepositoryConfigurationExtension
RepositoryConfigurationDelegate ..> AnnotationRepositoryConfigurationSource
RepositoryConfigurationDelegate ..> DefaultRepositoryConfiguration
RepositoryConfigurationDelegate ..> RepositoryBeanDefinitionBuilder
RepositoryConfigurationExtension <|.. MongoRepositoryConfigurationExtension
RepositoryConfigurationSourceSupport <|-- AnnotationRepositoryConfigurationSource
RepositoryConfigurationSource <|.. RepositoryConfigurationSourceSupport
RepositoryFactoryInformation <|.. RepositoryFactoryBeanSupport
FactoryBean <|.. RepositoryFactoryBeanSupport
RepositoryFactoryBeanSupport <|-- MongoRepositoryFactoryBean


